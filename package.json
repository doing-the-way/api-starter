{
  "name": "api-starter",
  "version": "1.0.0",
  "description": "API Starter",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --exec npm run restart",
    "restart": "rimraf dist && npm run build && npm run start",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.7",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.2",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "jest": "^25.2.4",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/test-db-setup.js"
    ],
    "testPathIgnorePatterns": [
      "dist/"
    ],
    "restoreMocks": true
  },
  "keywords": [],
  "author": "Franz Victorio <evenhold@gmail.com>",
  "license": "ISC"
}
